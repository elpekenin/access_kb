{
    "repo": "https://github.com/qmk/qmk_firmware",
    "branch": "xap",
    "keyboard": "elpekenin/access",
    "keymap": "elpekenin",
    "path": "$QMK_BUILD",
    "default_compilation": false,
    "operations": [
        {
            "operation": "cp",
            "orig": "keyboards/elpekenin/access/*",
            "dest": "keyboards/elpekenin/access"
        },
        {
            "operation": "cp",
            "orig": "$USERSPACE/*",
            "dest": "users/elpekenin"
        },
        {
            // 3color eink
            "operation": "pr",
            "id": 20032
        },
        {
            "operation": "checkout",
            "files": [
                "drivers/painter/comms/qp_comms_spi.c",
                "drivers/painter/generic/qp_surface_mono1bpp.c",

                "quantum/painter/qff.c", "quantum/painter/qff.h",
                "quantum/painter/qgf.c", "quantum/painter/qgf.h",

                "quantum/painter/qp_draw.h",
                "quantum/painter/qp_draw_core.c",
                "quantum/painter/qp_draw_ellipse.c",
                "quantum/painter/qp_draw_image.c",
                "quantum/painter/qp_draw_text.c",
                "quantum/painter/qp_internal.h",
                "quantum/painter/qp_internal_formats.h"

            ],
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "feat/qp-transparent"
        },
        {
            "operation": "checkout",
            "files": ["drivers/painter/il91874"],
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "il91874"
        },
        {
            "operation": "diff",
            "file": "il91874.diff"
        },
        {
            "operation": "diff",
            "file": "spi_comms.diff"
        },
        {
            "operation": "diff",
            "file": "generic_rotation.diff"
        },
        {
            // #20645(ish)
            "operation": "diff",
            "file": "native_color_font.diff"
        },
        {
            "operation": "diff",
            "file": "print_no_console.diff"
        },
        {
            "operation": "diff",
            "file": "xap_keytester.diff"
        },
        {
            "operation": "diff",
            "file": "eink_transparent.diff"
        },
        // {
        //     "operation": "diff",
        //     "file": "print_palette.diff"
        // },
        {
            "operation": "diff",
            "file": "xap_dynamic.diff"
        },
        {
            "operation": "exec",
            "command": "keyboards/elpekenin/access/compile",
            "at": "$QMK_BUILD"
        }
    ],
    "post_compilation": [
        {
            "operation": "exec",
            "command": "cp binaries/* /mnt/d"
        }
    ]
}