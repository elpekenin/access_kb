pub(crate)fn on_init() {
    match std::process::Command::new("sh")
            .arg("-c")
            .arg("sudo systemctl start docker && cd $HOME/docker  && docker compose up -d")
            .output()
    {
        Ok(_) => error!("on_init went correctly"),
        Err(out) => error!("on_init failed due to: {out}")
    }
}
